{
  "expo": {
    "name": "tick-it",
    "slug": "tick-it",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/tick-it-logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "tick-it",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kaiteur1967.tickit"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "51da268e-d488-405b-afd5-b69ea638afce"
      }
    },
    "owner": "kaiteur1967"
  }
}
